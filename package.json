{
  "name": "gatsby-starter-blog",
  "private": true,
  "description": "A starter for a blog powered by Gatsby and Markdown",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "gatsby": "^3.2.1",
    "gatsby-plugin-emotion": "^6.2.0",
    "gatsby-plugin-feed": "^3.2.0",
    "gatsby-plugin-gatsby-cloud": "^2.2.0",
    "gatsby-plugin-google-analytics": "^3.2.0",
    "gatsby-plugin-image": "^1.5.0",
    "gatsby-plugin-layout": "^2.3.0",
    "gatsby-plugin-manifest": "^3.2.0",
    "gatsby-plugin-mdx": "^2.2.0",
    "gatsby-plugin-offline": "^4.2.0",
    "gatsby-plugin-react-helmet": "^4.2.0",
    "gatsby-plugin-sass": "^4.2.0",
    "gatsby-plugin-sharp": "^3.5.0",
    "gatsby-remark-images": "^5.2.0",
    "gatsby-remark-prismjs": "^4.2.0",
    "gatsby-source-contentful": "^5.2.1",
    "gatsby-source-filesystem": "^3.5.0",
    "gatsby-transformer-sharp": "^3.5.0",
    "match-sorter": "^6.3.0",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "sass": "^1.32.8",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.75"
  },
  "devDependencies": {
    "eslint": "^7.27.0",
    "eslint-config-kentcdodds": "^18.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.2.1"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby-starter-blog#readme",
  "keywords": [
    "kentcdodds/jsx-a11y",
    "gatsby"
  ],
  "license": "0BSD",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gatsbyjs/gatsby-starter-blog.git"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "develop:debug": "node --inspect-brk  ./node_modules/.bin/gatsby develop",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|less|ts|tsx|graphql|md|mdx)\"",
    "start": "npm run develop",
    "clean": "gatsby clean",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/*.js": "eslint --cache --fix",
    "*.+(js|jsx|json|yml|yaml|less|scss|ts|tsx|graphql|md|mdx)": "prettier --write"
  }
}
